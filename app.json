{
  "name": "billy",
  "description": "billy",
  "repository": "https://github.com/koikijs/billy",
  "keywords": [
    "hubot"
  ],
  "env": {
    "HEROKU_URL": {
      "value": "https://billy.herokuapp.com"
    },
    "HUBOT_HEROKU_KEEPALIVE_URL": {
      "value": "https://billy.herokuapp.com/"
    },
    "HUBOT_HEROKU_SLEEP_TIME": {
      "value": "00:00"
    },
    "HUBOT_HEROKU_WAKEUP_TIME": {
      "value": "09:00"
    },
    "TZ": {
      "value": "Asia/Tokyo"
    },
    "HUBOT_SLACK_TOKEN": {
      "description": "Please specify slack token"
    }
  },
  "addons": [
    "scheduler:standard",
    "rediscloud:30"
  ]
}
